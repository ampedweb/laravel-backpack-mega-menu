{
    "name": "ampedweb/laravel-backpack-mega-menu",
    "description": "Mega menu builder for Laravel Backpack",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Powell",
            "email": "hosting@ampedwebdesign.co.uk",
            "homepage": "ampedweb.dev"
        }
    ],
    "homepage": "https://github.com/ampedweb/laravel-backpack-mega-menu",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "LaravelBackpackMegaMenu"
    ],
    "require": {
        "illuminate/support": "~7|~8",
        "backpack/crud": "^4.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Ampedweb\\LaravelBackpackMegaMenu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ampedweb\\LaravelBackpackMegaMenu\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ampedweb\\LaravelBackpackMegaMenu\\AddonServiceProvider"
            ]
        }
    }
}
